SQL DATABASE:


CREATE TABLE USER1
(
	USERID INT NOT NULL IDENTITY(1,1),
	USERNAME VARCHAR(50) NOT NULL, 
	PASSWORD VARCHAR(50) NOT NULL,
	CONSTRAINT PK_USERNAME PRIMARY KEY (USERNAME),

)

SELECT * FROM USER1


CREATE TABLE STUDYINFO1
(
	STUDYINFOID INT NOT NULL IDENTITY(1,1) ,
	USERNAME VARCHAR(50) NOT NULL,
	SEMESTER_DATE DATE NOT NULL, 
	MOD_CODE VARCHAR(50) NOT NULL, 
	MOD_NAME VARCHAR(255) NOT NULL, 
	MOD_CREDITS INT NOT NULL, 
	MOD_HPW DECIMAL NOT NULL, 
	MOD_NUMWEEKS DECIMAL NOT NULL, 
	STUD_HPW DECIMAL NOT NULL, 
	SELF_STUDHPW DECIMAL NOT NULL, 
	CALC_HPW DECIMAL NOT NULL, 

	CONSTRAINT FK_USER1 FOREIGN KEY (USERNAME)
    REFERENCES USER1(USERNAME),
	CONSTRAINT PK_STUDYINFOID1 PRIMARY KEY (STUDYINFOID)
    
) 



SELECT * FROM STUDYINFO1
